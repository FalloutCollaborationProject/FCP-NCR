            <Insulation_Heat>2</Insulation_Heat>
            <EquipDelay>1.5</EquipDelay>
        </statBases>
        <equippedStatOffsets>
            <ToxicEnvironmentResistance MayRequire="Ludeon.RimWorld.Biotech">0.90</ToxicEnvironmentResistance>
