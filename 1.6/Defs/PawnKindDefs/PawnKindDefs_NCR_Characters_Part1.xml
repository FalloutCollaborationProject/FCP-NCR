        <defName>FCP_Unique_Pawnkind_President_Kimball</defName>
        <label>President Kimball</label>
        <labelPlural>President Kimball</labelPlural>
        <race>Human</race>
        <combatPower>150</combatPower>
        <defaultFactionType>FCP_Faction_NCR</defaultFactionType>
