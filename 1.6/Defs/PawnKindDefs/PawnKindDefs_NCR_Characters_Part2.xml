    <!-- Chief Hanlon -->
    <PawnKindDef ParentName="FCP_NCRkindBase">
        <defName>FCP_Unique_Pawnkind_Chief_Hanlon</defName>
        <label>Chief Hanlon</label>
        <labelPlural>Chief Hanlon</labelPlural>
        <race>Human</race>
        <combatPower>140</combatPower>
        <defaultFactionType>FCP_Faction_NCR</defaultFactionType>
        <backstoryCryptosleepCommonality>0</backstoryCryptosleepCommonality>
        <itemQuality>Excellent</itemQuality>
        <isFighter>true</isFighter>
        <apparelMoney>2000~3000</apparelMoney>
        <apparelAllowHeadgearChance>1</apparelAllowHeadgearChance>
        <apparelIgnoreSeasons>true</apparelIgnoreSeasons>
        <weaponMoney>1000~2000</weaponMoney>
